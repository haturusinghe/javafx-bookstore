		<queryString>
			<![CDATA[SELECT
    book.title,
    order_details.quantity,
    order_details.unit_price,
    (
        order_details.quantity * order_details.unit_price
    ) AS subTotal
FROM
    orders,
    order_details,
    book
WHERE
    orders.order_id = order_details.order_id AND book.book_id = order_details.book_id AND orders.order_id = $P{currentId}]]>
