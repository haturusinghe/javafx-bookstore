<VBox prefHeight="685.0" prefWidth="1030.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="fct.cs.GenReport.GenerateReportController">
    <children>
        <JFXButton fx:id="generateBtn" minHeight="25.0" minWidth="100.0" onAction="#generateMonthly"
                   styleClass="custom-buttons-1" text="Generate Reports">
            <graphic>
                <FontIcon fill="BLACK" iconLiteral="anto-plus-circle" iconSize="20" selectionFill="BLACK"/>
            </graphic>
        </JFXButton>
        <JFXButton fx:id="yearlyBtn" minHeight="25.0" minWidth="100.0" onAction="#generateYearly"
                   styleClass="custom-buttons-1" text="Generate Yearly">
            <graphic>
                <FontIcon fill="BLACK" iconLiteral="anto-plus-circle" iconSize="20" selectionFill="BLACK"/>
            </graphic>
        </JFXButton>
    </children>
</VBox>
